<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        <Button
                android:text="Prawo Ohma"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button1"
                android:onClick="onClickPrawoOhma"/>

        <Button
                android:text="Moc Czynna"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button2"
                android:onClick="onClickObliczanieMocy"/>

        <Button
                android:text="Obliczanie spadków napięcia"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button4"
                android:onClick="onClickSpadekU"/>

        <Button
                android:text="Konwersja jednostek"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button5"
                android:onClick="onClickKonwersja"/>

        <Button
                android:text="Wybór przewodu"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button6"
                android:onClick="onClickWybor"/>

        <Button
                android:text="Miedzi w kablu"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button7"
                android:onClick="onClickMiedz"/>

        <Button
                android:text="Obliczanie oświetlenia"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button8"
                android:onClick="onClickOswietlenie"/>

        <Button
                android:text="Kalkulator czasu pracy"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button9"
                android:onClick="onClickKCP"/>

        <Button
                android:text="Reaktancja"
                android:layout_width="341dp"
                android:layout_height="wrap_content"
                android:id="@+id/button10"
                android:onClick="onClickButton1"/>

        <Button
                android:text="Obliczanie ochrony przeciwprzepięciowej"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button11"
                android:onClick="onClickOchrP"/>



    </LinearLayout>
</ScrollView>
